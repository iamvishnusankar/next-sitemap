{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "postbuild": {
      "dependsOn": ["^postbuild"],
      "outputs": [
        "dist/**",
        ".next/**",
        "public/robots.txt",
        "public/sitemap*.xml"
      ]
    },
    "deploy": {
      "dependsOn": ["^build", "postbuild"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
